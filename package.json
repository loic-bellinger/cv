{
  "name": "cv",
  "type": "module",
  "version": "0.0.1",
  "packageManager": "pnpm@10.28.1",
  "engines": {
    "node": ">=25.2.1",
    "pnpm": ">=10.28.1"
  },
  "devEngines": {
    "runtime": {
      "name": "node",
      "version": ">=25.2.1",
      "onFail": "error"
    },
    "packageManager": {
      "name": "pnpm",
      "version": ">=10.28.1",
      "onFail": "error"
    }
  },
  "scripts": {
    "dev": "astro dev",
    "check": "pnpm exec biome check .",
    "check:fix": "pnpm exec biome check --write .",
    "check:staged": "pnpm exec biome check --staged --no-errors-on-unmatched",
    "build": "pnpm run check && astro check && astro build",
    "preview": "astro preview",
    "astro": "astro",
    "prepare": "husky"
  },
  "dependencies": {
    "@astrojs/sitemap": "^3.7.0",
    "astro": "^5.16.9"
  },
  "devDependencies": {
    "@astrojs/check": "^0.9.6",
    "@biomejs/biome": "^2.3.11",
    "husky": "^9.1.7"
  }
}
